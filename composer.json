{
	"name": "etech/sms",
	"description": "CAMOO API Packages",
	"type": "library",
	"require": {
		"php": ">=8.0",
		"vlucas/valitron": "^1.4",
		"ext-mbstring": "*",
		"giggsey/libphonenumber-for-php": "^8.10",
		"camoo/curl-http-client": "^1.0"
	},
	"minimum-stability": "dev",
	"homepage": "https://camoo.cm",
	"license": "MIT",
	"prefer-stable" : true,
	"require-dev": {
        "phpunit/phpunit": "^9.0"
	},
	"authors": [
		{
			"name": "CAMOO SARL",
			"email": "sms@camoo.sarl"
		}
	],
	"keywords" : [
		"CAMOO",
		"SMS",
		"BULK SMS",
		"SMS Gateway",
		"Plugin camoo sms",
		"API-SMS"
	],
	"autoload": {
		"psr-4": {
			"Etech\\Sms\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "CamooSms\\Test\\": "tests"
        }
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
    },
	"scripts" : {
		"test": "phpunit --colors=always"
	},
	"support": {
		"issues": "https://github.com/camoo/etech-keys-sms/issues",
		"source": "https://github.com/camoo/etech-keys-sms",
		"wiki": "https://github.com/camoo/etech-keys-sms/wiki"
	}
}
